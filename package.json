{
  "name": "series-notifier-serverless",
  "version": "1.0.0",
  "description": "A facebook messenger bot that notifies you when you favorite series airs a new episode",
  "main": "src/index.js",
  "scripts": {
    "build": "rimraf build && webpack",
    "webpack:stats": "rimraf build && NODE_ENV=production webpack -p --profile --json > webpack-stats.json",
    "predeploy:function": "yarn build",
    "deploy:function": "sls deploy function -f",
    "predeploy:all": "yarn build",
    "deploy:all": "sls deploy --verbose",
    "preinvoke": "yarn build",
    "invoke": "sls invoke local -f messenger -p event.json",
    "lint": "tslint src/**/*.ts",
    "test": "NODE_ENV=test jest --config=jest.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaydp17/series-notifier-serverless.git"
  },
  "author": "Jaydeep Solanki <jaydp17@gmail.com> (https://jaydp.com)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/jaydp17/series-notifier-serverless/issues"
  },
  "homepage": "https://github.com/jaydp17/series-notifier-serverless#readme",
  "devDependencies": {
    "@types/bluebird": "3.5.0",
    "@types/jest": "^19.2.2",
    "@types/node": "^6.0.66",
    "awesome-typescript-loader": "^3.1.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.2.2",
    "jest": "^19.0.2",
    "prettier": "^0.22.0",
    "rimraf": "^2.6.1",
    "serverless": "^1.10.0",
    "ts-jest": "^19.0.8",
    "tslint": "^4.5.1",
    "tslint-microsoft-contrib": "^4.0.1",
    "typescript": "2.2.1",
    "webpack": "^2.3.2"
  },
  "dependencies": {
    "axios": "^0.16.0",
    "bluebird": "3.5.0"
  }
}
